	Name = "CRUCIFIX ESP",
	Default = false,
	Callback = function(v)

        if v then
            _G.Crucifix = true
        else
            _G.Crucifix = false
        end
	end,
})

_G.Aurant = false
HideCommonToggleKey = Enum.KeyCode.F4
Meshes = {
	--[16657069] = "Money Bag",
	["rbxassetid://5603610480"] = "AURANT",


	
